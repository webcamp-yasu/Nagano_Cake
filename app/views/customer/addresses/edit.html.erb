<h4>配送先編集ページ</h4>

<div>
	<%= form_for @address, url: address_path do |f| %>
	<%= f.label :"郵便番号(ハイフンなし)" %>
	<%= f.text_field :postal_code %>
</div>

<div>
	<%= f.label :"住所" %>
	<%= f.text_field :address %>
</div>

<div class="form-inline">
	<%= f.label :"宛名" %>
	<%= f.text_field :name %>
	<%= f.submit "編集する", class: "btn btn-primary" %>
	<% end %>
</div>