<h1>注文履歴一覧</h1>
<table>
	<thead>
		<tr>
			<th>注文日</th>
			<th>配送先</th>
			<th>注文商品</th>
			<th>支払金額</th>
			<th>ステータス</th>
			<th>注文詳細</th>
		</tr>
	</thead>
	<tbody>
		<% @orders .each do |order| %>
		<tr>
			<td><%= order.created_at %></td>
			<td><%= order.postal_code %><%= order.address %><%= order.address_name %></td>
			<td><%= order. %></td>
			<td><%= order. %></td>
			<td><%= order.status %></td>
			<td><%= link_to order_path %></td>
		</tr>
		<% end %>
	</tbody>
</table>